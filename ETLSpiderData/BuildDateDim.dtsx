              contactInfo="通过使用相应的 .NET Framework 数据访问接口来使用 SQL Server、OLE DB、ODBC 或 Oracle 中的数据。使用 Transact-SQL 语句定义结果集。例如，使用适用于 SQL Server 的 .NET Framework 数据访问接口从 SQL Server 中提取数据。;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; 保留所有权利; http://www.microsoft.com/sql/support;4"
              description="通过使用相应的 .NET Framework 数据访问接口来使用 SQL Server、OLE DB、ODBC 或 Oracle 中的数据。使用 Transact-SQL 语句定义结果集。例如，使用适用于 SQL Server 的 .NET Framework 数据访问接口从 SQL Server 中提取数据。"
              name="GetDateData"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="指定组件用来提取数据的 SQL 语句。"
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT [PK_日期] Datetime,
       Convert(int,CONVERT(varchar(12),[PK_日期],112)) AS DateKey,
	   DATEPART(YEAR,[PK_日期]) AS DYear,
	   DATEPART(MONTH,[PK_日期]) AS MonthOfYear,
	   DATEPART(DAY,[PK_日期]) AS DaysOfMonth,
	   DATEPART(QUARTER,[PK_日期]) AS QuaterOfYear,
	   DATEPART(WEEK,[PK_日期]) AS WeeksOfYear,
	   DATEPART(WEEKDAY,[PK_日期]) AS DaysOfWeek,
	   DATENAME(DAYOFYEAR,[PK_日期]) AS DDaysOfYear,
	   DATENAME(WEEKDAY,[PK_日期]) AS Weekend,
	   CASE 
	   WHEN DATEPART(MONTH,[PK_日期]) &gt; 6 THEN N'下半年'
	   ELSE N'上半年'
	   END AS HalfYear,
	   DATENAME(YEAR,[PK_日期]) + N'年' + DATENAME(QUARTER,[PK_日期]) + N'季度' AS QuatersName,
	   DATENAME(YEAR,[PK_日期]) + N'年' + DATENAME(QUARTER,[PK_日期]) + N'季度' + DATENAME(MONTH,[PK_日期]) + N'月' AS MonthsName,
	   DATENAME(YEAR,[PK_日期]) + N'年' + DATENAME(QUARTER,[PK_日期]) + N'季度' + DATENAME(MONTH,[PK_日期]) + N'月' + DATENAME(DAY,[PK_日期]) + N'号' AS DaysName
  FROM [ZhaopinSpiderData].[dbo].[DateDim]</property>
