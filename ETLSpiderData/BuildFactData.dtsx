              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="通过使用相应的 .NET Framework 数据访问接口来使用 SQL Server、OLE DB、ODBC 或 Oracle 中的数据。使用 Transact-SQL 语句定义结果集。例如，使用适用于 SQL Server 的 .NET Framework 数据访问接口从 SQL Server 中提取数据。;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; 保留所有权利; http://www.microsoft.com/sql/support;4"
              description="通过使用相应的 .NET Framework 数据访问接口来使用 SQL Server、OLE DB、ODBC 或 Oracle 中的数据。使用 Transact-SQL 语句定义结果集。例如，使用适用于 SQL Server 的 .NET Framework 数据访问接口从 SQL Server 中提取数据。"
              name="GetFactData"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="指定组件用来提取数据的 SQL 语句。"
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
       [title_name]
      ,[company_name]
      ,[title_salary]
      ,[company_address]
      ,[issuer_date]
      ,[work_experience]
      ,[education_background]
      ,[company_scale]
      ,[company_type]
      ,rtrim(ltrim(REPLACE([job_introuduce],CHAR(13) + CHAR(10),''))) AS job_introduce
      ,[job_tag]
      ,[page_num]
      ,[index_num]
      ,CONVERT(datetime,CONVERT(date,[crawl_date]))crawl_date
      ,[job_sub_tag]
	  ,CASE CHARINDEX('-',title_salary)
	   WHEN 0 THEN '0'
	   ELSE SUBSTRING(title_salary,0,CHARINDEX('-',title_salary))
	   END AS MinSalary
	  ,CASE CHARINDEX('元',title_salary)
	   WHEN 0 THEN '0'
	   ELSE SUBSTRING(title_salary,CHARINDEX('-',title_salary)+1,CHARINDEX('元',title_salary)-CHARINDEX('-',title_salary)-1)
	   END AS MaxSalary
       ,job_tag + '-' +job_sub_tag AS full_title
      ,SUBSTRING(education_background,CHARINDEX(':',education_background)+1,8) + SUBSTRING(work_experience,CHARINDEX(':',work_experience)+1,8) AS RequireKey
  FROM [ZhaopinSpiderData].[dbo].[JobInformation]
 WHERE CONVERT(datetime,crawl_date) &gt; '2017-12-1' AND CONVERT(datetime,crawl_date) &lt; '2018-1-31'</property>
